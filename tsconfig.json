{
    "compilerOptions": {
        "target": "es5",
        "outDir": "bin-debug",
        "declaration": false,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictFunctionTypes": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "lib": [
            "es6",
            "es7",
            "dom"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}